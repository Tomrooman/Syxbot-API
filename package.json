{
  "name": "syxbot-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start-log": "npm-run-all build server log-prod",
    "start": "npm-run-all build server",
    "server": "pm2 start dist/index.js --name syxbot_prod_api",
    "dev": "pm2 start runner.js --watch --name syxbot_api && npm run log-dev",
    "test": "mocha ./lib/test/runner.js --exit",
    "log-dev": "pm2 log --lines 100 syxbot_api ",
    "log-prod": "pm2 log --lines 100 syxbot_prod_api",
    "eslint": "eslint ./",
    "status": "pm2 status",
    "transpile": "babel ./ --out-dir dist --copy-files --ignore node_modules,lib/test,runner.js,babel.config.json,UPDATER.js,package.json,yarn.lock,package-lock.json,.yarn,.pnp.js,.yarnrc.yml",
    "clean": "rimraf dist",
    "clean-unusable": "rimraf dist/node_modules dist/lib/test dist/runner.js dist/babel.config.json dist/UPDATER.js dist/package.json dist/yarn.lock dist/package-lock.json dist/.yarn dist/.pnp.js dist/.yarnrc.yml",
    "remove-modules": "yarn rimraf .yarn/build-state.yml .yarn/cache .yarn/install-state.gz .yarn/unplugged .pnp.js yarn.lock package-lock.json",
    "build": "npm-run-all clean transpile clean-unusable",
    "stop-dev": "pm2 stop syxbot_api",
    "stop-prod": "pm2 stop syxbot_prod_api",
    "stop-all": "pm2 stop all",
    "mongodb": "sudo service mongod start"
  },
  "private": true,
  "author": "Tom Rooman",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "dateformat": "^3.0.3",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.23",
    "nodemailer": "^6.4.10",
    "npm-run-all": "^4.1.5",
    "querystring": "^0.2.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "mocha": "^8.0.1"
  }
}
